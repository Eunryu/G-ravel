<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:choiceMode="singleChoice"
    android:divider="@android:color/transparent"
    xmlns:tools="http://schemas.android.com/tools"
    >
    <Button 
        android:layout_width="fill_parent"
        android:layout_height="48dp"
        android:id="@+id/btn_AllSidemenu_Notice"
        android:text="@string/Side_notice"
        />
    <Button 
        android:layout_width="fill_parent"
        android:layout_height="48dp"
        android:id="@+id/btn_AllSidemenu_NumberList"
        android:text="@string/Side_numberList"
        />
    <Button 
        android:layout_width="fill_parent"
        android:layout_height="48dp"
        android:id="@+id/btn_AllSidemenu_Daily"
        android:text="@string/Side_daily"
        />
    <Button 
        android:layout_width="fill_parent"
        android:layout_height="48dp"
        android:id="@+id/btn_AllSidemenu_FreeBoard"
        android:text="@string/Side_freeboard"
        />
    <Button 
        android:layout_width="fill_parent"
        android:layout_height="48dp"
        android:id="@+id/btn_AllSidemenu_Setting"
        android:text="@string/Side_setting"
        />

</LinearLayout>
